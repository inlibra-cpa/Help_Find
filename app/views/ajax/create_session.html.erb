<div id="session_rest_operations_div">

  <!-- Help Find API Create User -->
  <b><p>Create User</p></b>

  <label>User Email</label>
  <input id="create_user_email">

  <br>

  <label>User Password</label>
  <br>
  <!-- Create User Session: Toggle Password Visibility -->
  <input type="password" id="create_user_password"><br><br>
  <input type="checkbox" onclick="create_user_password_function()">Show Password

  <script>
  function create_user_password_function() {
    var x = document.getElementById("create_user_password");
    if (x.type === "password") {
      x.type = "text";
    } else {
      x.type = "password";
    }
  }
  </script>

  <br>
  <br>

  <button id="create_user_button" type="button" a href="#" class="button">Create</button>

  <br>

